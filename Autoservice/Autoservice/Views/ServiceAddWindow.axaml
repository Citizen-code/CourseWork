<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Autoservice.ServiceAddWindow"
		Background="#9191E9"
        Title="ServiceAddWindow">
	<Canvas>
		<TextBox Name="ServiceNameTextBox"
				 Canvas.Left="180"
				 Canvas.Top="160"
				 Height="20"
				 Width="200"
				 Background="#D9D9D9"
				 CornerRadius="2"/>
		<TextBox Name="CostTextBox"
				 Canvas.Right="180"
				 Canvas.Top="160"
				 Height="20"
				 Width="200"
				 Background="#D9D9D9"
				 CornerRadius="2"/>
		<CheckBox Name="IsHourlyCheckBox"
				  Canvas.Left="280"
				  Canvas.Top="200"
				  Content="Почасовая"/>
		<Button Name="AddServiceButton"
				Canvas.Left="260"
				Canvas.Top="250"
				Height="35"
				Width="120"
				Background="#C2AFF0"
				Content="ДОБАВИТЬ"
				Click="AddServiceButton_Click"/>
		<Button Name="BackButton"
				Canvas.Right="260"
				Canvas.Top="250"
				Height="35"
				Width="120"
				Background="#E23A3A"
				Content="НАЗАД"
				Click="BackButton_Click"/>
	</Canvas>
</Window>
